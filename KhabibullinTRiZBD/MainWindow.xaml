<Window x:Class="KhabibullinTRiZBD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KhabibullinTRiZBD"
        mc:Ignorable="d"
        Title="InformationayaSystema" Height="450" Width="800" Background="MediumTurquoise" Icon="Res/Icon.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="Res/Logo.png" HorizontalAlignment="Left" VerticalAlignment="Bottom"></Image>
        <TextBlock Text="Преподаватели" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="Right"  Click="BtnBack_Click"></Button>
    </Grid>
</Window>
